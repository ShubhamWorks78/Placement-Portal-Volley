<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.seals.shubham.placementportalvolley"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".RegistrationActivity">
        </activity>
        <activity android:name=".ForgotPassword">
        </activity>
        <activity
            android:name=".FeedActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable"/>
        </activity>
        <activity android:name=".PasswordResettedAcknowledgement">
        </activity>
        <activity android:name=".AdminOrNormal">
        </activity>
        <activity android:name=".PasswordReset">
        </activity>
        <activity android:name=".UploadActivity">
        </activity>
        <activity android:name=".PlacementStats">
        </activity>
        <activity android:name=".About_Description">
        </activity>
    </application>

</manifest>